			<F N="public/robots.txt"/>
		</Folder>
		<Folder Name="script">
			<Folder Name="performance">
				<F N="script/performance/benchmarker"/>
				<F N="script/performance/profiler"/>
			</Folder>
			<Folder Name="process">
				<F N="script/process/reaper"/>
