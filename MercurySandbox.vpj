				<F N="config/environments/development.rb"/>
				<F N="config/environments/production.rb"/>
				<F N="config/environments/test.rb"/>
			</Folder>
			<F N="config/boot.rb"/>
			<F N="config/database.yml"/>
			<F N="config/environment.rb"/>
			<F N="config/routes.rb"/>
		</Folder>
		<Folder Name="db">
			<Folder Name="migrate">
				<F N="db/migrate/001_create_items.rb"/>
				<F N="db/migrate/002_create_title_chars.rb"/>
				<F N="db/migrate/003_create_title_char_ancestors.rb"/>
				<F N="db/migrate/004_create_item_title_chars.rb"/>
